.container {
  display: inline-flex;
  flex-direction: row;
  height: 60rpx;
}
.index-container {
  display: flex;
  flex-direction: row;
  align-items: baseline;
}
.plain {
  font-size: 32rpx;
}
.index {
  margin-right: 14rpx;
  line-height: 60rpx;
  font-size: 60rpx;
  font-weight: 800;
}
.line {
  height: 44rpx;
  margin-right: 14rpx;
  border-left: 1px solid black;
}
.date-container {
  display: flex;
  flex-direction: column;
  margin-top: 6rpx;
}
.month {
  font-size: 24rpx;
  line-height: 24rpx;
}
.year {
  font-size: 20rpx;
}